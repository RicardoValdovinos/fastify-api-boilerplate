{
  "name": "fastify-api-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "setup": "git init && husky install",
    "format": "prettier src/**/*.ts --write",
    "lint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0 --fix",
    "dev": "tsx src/server.ts",
    "test": "tsx test/index.ts",
    "test:coverage": "c8 tsx test/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "clean": "rm -rf dist",
    "commitizen": "exec < /dev/tty && git cz --hook || true",
    "commitlint": "commitlint --edit"
  },
  "keywords": [],
  "author": "Ricardo Valdovinos <ricardovaldovinos1996@gmail.com>",
  "license": "MIT",
  "engine": "18.16.0",
  "dependencies": {
    "@auth/core": "^0.26.3",
    "@fastify/autoload": "^5.8.0",
    "@fastify/caching": "^8.3.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/csrf-protection": "^6.4.1",
    "@fastify/env": "^4.3.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@sinclair/typebox": "^0.32.13",
    "close-with-grace": "^1.2.0",
    "fastify": "^4.26.0",
    "fastify-plugin": "^4.5.1",
    "kysely": "^0.27.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "c8": "^9.1.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "fastify-print-routes": "^3.1.1",
    "husky": "^9.0.10",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
