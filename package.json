{
  "name": "fastify-api-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "setup": "git init && husky install",
    "format": "prettier src/**/*.ts --write",
    "lint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0 --fix",
    "dev": "tsx src/server.ts",
    "test": "tsx test/index.ts",
    "test:coverage": "c8 tsx test/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "clean": "rm -rf dist",
    "commitizen": "exec < /dev/tty && git cz --hook || true",
    "commitlint": "commitlint --edit"
  },
  "keywords": [],
  "author": "Ricardo Valdovinos <ricardovaldovinos1996@gmail.com>",
  "license": "MIT",
  "engine": "18.16.0",
  "dependencies": {
    "@auth/core": "^0.34.2",
    "@fastify/autoload": "^5.10.0",
    "@fastify/caching": "^8.4.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/csrf-protection": "^6.4.1",
    "@fastify/env": "^4.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.0.1",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@sinclair/typebox": "^0.32.35",
    "close-with-grace": "^1.3.0",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "kysely": "^0.27.4",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^22.1.0",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "c8": "^10.1.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^17.10.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "fastify-print-routes": "^3.2.0",
    "husky": "^9.1.4",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
