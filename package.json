{
  "name": "fastify-api-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "setup": "git init && husky install",
    "format": "prettier src/**/*.ts --write",
    "lint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0 --fix",
    "dev": "tsx src/server.ts",
    "test": "tsx test/index.ts",
    "test:coverage": "c8 tsx test/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "clean": "rm -rf dist",
    "commitizen": "exec < /dev/tty && git cz --hook || true",
    "commitlint": "commitlint --edit"
  },
  "keywords": [],
  "author": "Ricardo Valdovinos <ricardovaldovinos1996@gmail.com>",
  "license": "MIT",
  "engine": "18.16.0",
  "dependencies": {
    "@auth/core": "^0.28.0",
    "@fastify/autoload": "^5.8.0",
    "@fastify/caching": "^8.3.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/csrf-protection": "^6.4.1",
    "@fastify/env": "^4.3.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/sensible": "^5.5.0",
    "@fastify/static": "^7.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@fastify/under-pressure": "^8.3.0",
    "@fastify/websocket": "^9.0.0",
    "@sinclair/typebox": "^0.32.15",
    "close-with-grace": "^1.3.0",
    "fastify": "^4.26.1",
    "fastify-plugin": "^4.5.1",
    "kysely": "^0.27.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.2",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "c8": "^9.1.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-unicorn": "^51.0.1",
    "fastify-print-routes": "^3.1.1",
    "husky": "^9.0.11",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
